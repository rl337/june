{
  "dashboard": {
    "title": "LLM (Inference API) Service",
    "tags": ["june", "llm", "inference", "service"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 0,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "Request Rate",
        "type": "timeseries",
        "gridPos": {"h": 6, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "sum(rate(inference_requests_total[5m])) by (status)",
            "refId": "A",
            "legendFormat": "{{status}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "decimals": 2,
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 2,
        "title": "Token Generation Rate",
        "type": "timeseries",
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(inference_token_generation_rate_bucket[5m]))",
            "refId": "A",
            "legendFormat": "p95 tokens/s"
          },
          {
            "expr": "histogram_quantile(0.50, rate(inference_token_generation_rate_bucket[5m]))",
            "refId": "B",
            "legendFormat": "p50 tokens/s"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "tokens/s",
            "decimals": 1,
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 3,
        "title": "Total Tokens Generated",
        "type": "timeseries",
        "gridPos": {"h": 6, "w": 12, "x": 0, "y": 6},
        "targets": [
          {
            "expr": "sum(rate(inference_tokens_total[5m])) by (model)",
            "refId": "A",
            "legendFormat": "{{model}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "tokens/s",
            "decimals": 1,
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 4,
        "title": "Context Usage",
        "type": "timeseries",
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 6},
        "targets": [
          {
            "expr": "inference_context_usage_tokens",
            "refId": "A",
            "legendFormat": "Context Tokens"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 0,
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 5,
        "title": "RAG Retrieval Time",
        "type": "timeseries",
        "gridPos": {"h": 6, "w": 12, "x": 0, "y": 12},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(inference_rag_retrieval_seconds_bucket[5m]))",
            "refId": "A",
            "legendFormat": "p95"
          },
          {
            "expr": "histogram_quantile(0.50, rate(inference_rag_retrieval_seconds_bucket[5m]))",
            "refId": "B",
            "legendFormat": "p50"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "decimals": 3,
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 6,
        "title": "Model Load Time",
        "type": "timeseries",
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 12},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(inference_model_load_seconds_bucket[5m]))",
            "refId": "A",
            "legendFormat": "p95"
          },
          {
            "expr": "histogram_quantile(0.50, rate(inference_model_load_seconds_bucket[5m]))",
            "refId": "B",
            "legendFormat": "p50"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "decimals": 2,
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 7,
        "title": "Inference Errors",
        "type": "timeseries",
        "gridPos": {"h": 6, "w": 12, "x": 0, "y": 18},
        "targets": [
          {
            "expr": "sum(rate(inference_errors_total[5m])) by (error_type)",
            "refId": "A",
            "legendFormat": "{{error_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "decimals": 2,
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 8,
        "title": "Active Connections",
        "type": "stat",
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 18},
        "targets": [
          {
            "expr": "inference_active_connections",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 0,
            "color": {"mode": "palette-classic"}
          }
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    }
  }
}
