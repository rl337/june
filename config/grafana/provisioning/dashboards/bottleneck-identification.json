{
  "dashboard": {
    "title": "Bottleneck Identification Dashboard",
    "tags": ["june", "performance", "bottleneck", "monitoring"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 0,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "Request Latency by Endpoint (p95)",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(gateway_response_time_seconds_bucket[5m])) by (le, endpoint))",
            "refId": "A",
            "legendFormat": "{{endpoint}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "decimals": 3,
            "color": {"mode": "palette-classic"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 1, "color": "yellow"},
                {"value": 2, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Service Response Times (p50, p95, p99)",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(gateway_response_time_seconds_bucket[5m])) by (le))",
            "refId": "A",
            "legendFormat": "p50"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(gateway_response_time_seconds_bucket[5m])) by (le))",
            "refId": "B",
            "legendFormat": "p95"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(gateway_response_time_seconds_bucket[5m])) by (le))",
            "refId": "C",
            "legendFormat": "p99"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "decimals": 3
          }
        }
      },
      {
        "id": 3,
        "title": "Database Query Times by Table",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(gateway_db_query_duration_seconds_bucket[5m])) by (le, table))",
            "refId": "A",
            "legendFormat": "{{table}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "decimals": 3,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 0.5, "color": "yellow"},
                {"value": 1.0, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Slow Database Queries (>1s)",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "sum(rate(gateway_db_slow_queries_total[5m])) by (table)",
            "refId": "A",
            "legendFormat": "{{table}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "decimals": 1
          }
        }
      },
      {
        "id": 5,
        "title": "Cache Hit/Miss Rates",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "sum(rate(cache_hits_total[5m])) by (cache_type)",
            "refId": "A",
            "legendFormat": "{{cache_type}} hits"
          },
          {
            "expr": "sum(rate(cache_misses_total[5m])) by (cache_type)",
            "refId": "B",
            "legendFormat": "{{cache_type}} misses"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "decimals": 1
          }
        }
      },
      {
        "id": 6,
        "title": "Cache Hit Rate Percentage",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "sum(rate(cache_hits_total[5m])) by (cache_type) / (sum(rate(cache_hits_total[5m])) by (cache_type) + sum(rate(cache_misses_total[5m])) by (cache_type)) * 100",
            "refId": "A",
            "legendFormat": "{{cache_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "decimals": 1,
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "red"},
                {"value": 50, "color": "yellow"},
                {"value": 80, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "title": "Connection Pool Utilization",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "gateway_db_pool_utilization",
            "refId": "A",
            "legendFormat": "{{pool_name}}"
          },
          {
            "expr": "gateway_db_pool_active_connections / gateway_db_pool_size",
            "refId": "B",
            "legendFormat": "Active/Total"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "decimals": 2,
            "min": 0,
            "max": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 0.7, "color": "yellow"},
                {"value": 0.9, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 8,
        "title": "GPU Utilization",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
        "targets": [
          {
            "expr": "inference_api_gpu_utilization_percent",
            "refId": "A",
            "legendFormat": "GPU {{gpu_id}} ({{gpu_name}})"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "decimals": 1,
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "red"},
                {"value": 50, "color": "yellow"},
                {"value": 80, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 9,
        "title": "GPU Memory Usage",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 32},
        "targets": [
          {
            "expr": "inference_api_gpu_memory_used_bytes / inference_api_gpu_memory_total_bytes * 100",
            "refId": "A",
            "legendFormat": "GPU {{gpu_id}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "decimals": 1,
            "min": 0,
            "max": 100
          }
        }
      },
      {
        "id": 10,
        "title": "Queue Depths",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 32},
        "targets": [
          {
            "expr": "telegram_queue_depth",
            "refId": "A",
            "legendFormat": "{{queue_name}} ({{queue_type}})"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 10, "color": "yellow"},
                {"value": 50, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 11,
        "title": "Queue Processing Time",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 40},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(telegram_queue_processing_time_seconds_bucket[5m])) by (le, queue_name))",
            "refId": "A",
            "legendFormat": "{{queue_name}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "decimals": 2
          }
        }
      },
      {
        "id": 12,
        "title": "Service Memory Usage",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 40},
        "targets": [
          {
            "expr": "gateway_memory_usage_bytes",
            "refId": "A",
            "legendFormat": "Gateway"
          },
          {
            "expr": "inference_api_memory_usage_bytes",
            "refId": "B",
            "legendFormat": "Inference API"
          },
          {
            "expr": "stt_memory_usage_bytes",
            "refId": "C",
            "legendFormat": "STT"
          },
          {
            "expr": "tts_memory_usage_bytes",
            "refId": "D",
            "legendFormat": "TTS"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bytes",
            "decimals": 0
          }
        }
      },
      {
        "id": 13,
        "title": "Service CPU Usage",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 48},
        "targets": [
          {
            "expr": "gateway_cpu_usage_percent",
            "refId": "A",
            "legendFormat": "Gateway"
          },
          {
            "expr": "inference_api_cpu_usage_percent",
            "refId": "B",
            "legendFormat": "Inference API"
          },
          {
            "expr": "stt_cpu_usage_percent",
            "refId": "C",
            "legendFormat": "STT"
          },
          {
            "expr": "tts_cpu_usage_percent",
            "refId": "D",
            "legendFormat": "TTS"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "decimals": 1,
            "min": 0,
            "max": 100
          }
        }
      },
      {
        "id": 14,
        "title": "Top Slow Endpoints",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 48},
        "targets": [
          {
            "expr": "topk(10, histogram_quantile(0.95, sum(rate(gateway_response_time_seconds_bucket[5m])) by (le, endpoint)))",
            "refId": "A",
            "format": "table",
            "instant": true
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "decimals": 3
          }
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    }
  }
}
